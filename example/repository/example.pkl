import "@ansipkl/playbook.pkl"
import "@ansipkl/builtin.pkl"
import "@ansipkl/community.general.pkl"

example_playbook_01 = new Listing<playbook.play> {
    new {
        name = "one"
        hosts = "self"
        remote_user = "root"
        tasks {
            new builtin.apt {
                name = "nginx"
                options {
                    name {
                        "nginx"
                    }
                }
            }.into()

            new `community.general`.pacman {
                name = "neovim"
                options {
                    name {
                        "neovim"
                    }
                }
            }.into()
        }
    }
}


